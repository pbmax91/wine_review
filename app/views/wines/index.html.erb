<h1>Wines</h1>
<p>Available as of <%= formatted_time(@available_at) %></p>
<ol>
	<% @wines.each do |wine| %>
	<li><b><%= link_to(wine.name, wine) %></b></li>
	<li><i>Winery:</i> <%= wine.winery %></li>
	<li><i>Varietal:</i> <%= wine.varietal %></li>
	<br>
	<% end %>
</ol>
<%= link_to 'New Wine', new_wine_path %>